# Cognito configuration
VITE_COGNITO_USER_POOL_ID=your-user-pool-id
VITE_COGNITO_CLIENT_ID=your-client-id

# API URL configuration
# - For local development: leave empty (Vite proxy handles /api requests)
# - For production build: set to empty string or the actual API endpoint
# - DO NOT use localhost for production builds!
VITE_API_URL=

# Mock auth - ONLY for local development without Cognito
# Set to false for production builds!
VITE_USE_MOCK_AUTH=false
